upload_txt <- function() {
cat("Please select the TXT file.\n")
filename <- file.choose()
# Check file extension
if (tolower(tools::file_ext(filename)) != "txt") {
stop("Unvalid filetype, please select .txt based file. Import aborted.")
}
cat("Importing data from:", filename, "\n")
raw_text <- readLines(filename, warn = FALSE)
# Split datasets by double line break
datasets <- unlist(strsplit(paste(raw_text, collapse = "\n"), "\n\n"))
data_list <- lapply(seq_along(datasets), function(i) {
df <- tryCatch({
temp_df <- read.table(text = datasets[i], header = FALSE, stringsAsFactors = FALSE, sep = "")
# Check dimensions
if (nrow(temp_df) != 8 || ncol(temp_df) != 12) {
stop("Datasets outside expected dimensions (8x12), check if each dataset is separated by double line break. Import aborted.")
}
# Convert to numeric and check for non-numeric values
temp_df[] <- lapply(temp_df, function(x) {
x <- gsub(",", ".", x)  # Convert commas to dots
if (any(is.na(as.numeric(x)))) {
stop("Non-numeric character detected, import aborted.")
}
return(as.numeric(x))
})
return(temp_df)
}, error = function(e) {
stop(e$message)  # Propagate error messages
})
return(df)
})
names(data_list) <- paste0("dataset_", seq_along(datasets))
cat("Data imported and stored.\n")
return(data_list)
}
